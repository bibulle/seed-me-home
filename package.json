{
  "name": "seed-me-home",
  "version": "0.4.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "DEBUG=server:* ng serve --sourceMap=true --evalSourceMap=true --vendorSourceMap=true",
    "start:backend": "DEBUG=server:* ng serve backend",
    "build": "ng build",
    "test": "ng test",
    "test:frontend:watch": "ng test frontend --watch --code-coverage",
    "test:backend:watch": "ng test backend --watch --code-coverage",
    "test:models:watch": "ng test models --watch --code-coverage",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "release": "standard-version --no-verify --commit-all",
    "replace:version": "json -I -f libs/models/src/lib/version.json -e \"this.version='`cat package.json | json version`';\" && git add libs/models/src/lib/version.json",
    "test:ci": "ng test"
  },
  "standard-version": {
    "scripts": {
      "postbump": "npm run replace:version"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "git status && test -z \"$(git status --porcelain)\" && npm run test:ci"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint -p tsconfig.json",
      "git add"
    ],
    "*.{css,scss,json,json5,md,js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@angular/animations": "^7.2.14",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.14",
    "@angular/compiler": "^7.2.14",
    "@angular/core": "^7.2.14",
    "@angular/forms": "^7.2.14",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.14",
    "@angular/platform-browser-dynamic": "^7.2.14",
    "@angular/router": "^7.2.14",
    "@auth0/angular-jwt": "^2.1.0",
    "@electorrent/node-rtorrent": "^1.2.1",
    "@nestjs/common": "^6.1.1",
    "@nestjs/core": "^6.1.1",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.1.1",
    "@nestjs/platform-ws": "^6.1.1",
    "@nestjs/websockets": "^6.1.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "core-js": "^2.5.4",
    "express-session": "^1.16.1",
    "moment": "^2.24.0",
    "ngx-logger": "^3.3.12",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "rxjs": "6.4.0",
    "socket.io": "^2.2.0",
    "ssh2": "^0.8.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "7.3.1",
    "@angular/compiler-cli": "^7.2.14",
    "@angular/language-service": "^7.2.14",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@nestjs/schematics": "^6.2.0",
    "@nestjs/testing": "^6.1.1",
    "@nrwl/builders": "^7.8.3",
    "@nrwl/schematics": "7.7.1",
    "@types/jest": "24.0.9",
    "@types/jquery": "3.3.6",
    "@types/node": "^8.10.46",
    "@types/passport-jwt": "^3.0.1",
    "codelyzer": "~4.5.0",
    "commitizen": "^3.1.1",
    "cypress": "^3.2.0",
    "dotenv": "6.2.0",
    "git-status-cli": "^0.7.0",
    "husky": "^1.3.1",
    "jest": "24.7.1",
    "jest-localstorage-mock": "^2.4.0",
    "jest-preset-angular": "^7.1.0",
    "json": "^9.0.6",
    "lint-staged": "^8.1.5",
    "prettier": "1.15.2",
    "replace": "^1.1.0",
    "rxjs-marbles": "^5.0.2",
    "standard-version": "^5.0.2",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "3.2.2"
  }
}
