{
  "name": "seed-me-home",
  "version": "0.2.4",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:backend:watch": "ng test backend --watch",
    "test:models:watch": "ng test models --watch",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "version": "standard-version --no-verify --commit-all",
    "postprepare": "npm run replace",
    "replace": "run-s replace:version",
    "replace:version": "json -I -f libs/models/src/lib/version.json -e \"this.version='`cat package.json | json version`';\" && git add libs/models/src/lib/version.json",
    "test:ci": "ng test --configuration=ci"
  },
  "standard-version": {
    "scripts": {
      "postbump": "npm run replace"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "git status && test -z \"$(git status --porcelain)\" && npm run test:ci && npm run version"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint -p tsconfig.json",
      "git add"
    ],
    "*.{css,scss,json,json5,md,js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@electorrent/node-rtorrent": "^1.2.1",
    "@nestjs/common": "5.5.0",
    "@nestjs/core": "5.5.0",
    "core-js": "^2.5.4",
    "rxjs": "6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "7.3.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "^7.0.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@nestjs/schematics": "5.11.2",
    "@nestjs/testing": "5.5.0",
    "@nrwl/builders": "7.7.1",
    "@nrwl/schematics": "7.7.1",
    "@types/jest": "24.0.9",
    "@types/jquery": "3.3.6",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "commitizen": "^3.0.7",
    "cypress": "^3.1.5",
    "dotenv": "6.2.0",
    "git-status-cli": "^0.7.0",
    "husky": "^1.3.1",
    "jest": "24.1.0",
    "jest-preset-angular": "7.0.0",
    "json": "^9.0.6",
    "lint-staged": "^8.1.5",
    "prettier": "1.15.2",
    "replace": "^1.1.0",
    "standard-version": "^5.0.1",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "3.2.2"
  }
}
